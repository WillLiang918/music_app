<h1>Create a Band</h1>

<%= render "shared/errors" %>

<form action="<%= bands_url %>" method="Post">
  <%= auth_token %>

  <label for="band_name">Band Name</label>
  <input  type="text"
          name="band[name]"
          id="band_name"
          value="<%= @band.name %>">

  <br>
  <input type="submit" value="Create Band!">
</form>
