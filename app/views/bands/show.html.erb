<h1>Band Profile<h2>

<form>
  <%= @band.name %>
  <br>
  <%= button_to "Edit", edit_band_url, method: :get %>
  <%= button_to "Delete", band_url, method: :delete %>
  <%= button_to "Create Album", new_band_album_url(params[:id]), method: :get %>

  <% @band.albums.each do |album| %>
    <%= link_to "#{album.name}" %>
  <% end %>

</form>
